import openpyxl

# Load the workbook and select the active sheet
file_path = 'output.xlsx'
workbook = openpyxl.load_workbook(file_path)
sheet = workbook.active  # or workbook['Sheet1'] if you know the sheet name

# Assuming the access token is in cell B2, adjust as needed
from dhanhq import dhanhq
client_id = sheet['C3'].value
access_token = sheet['E3'].value
token_expiry = sheet['G3'].value

print("Your token_expires:",token_expiry)
dhan = dhanhq(client_id, access_token)
